.custom{
    position: fixed;
    width: 100%;
    top: 0;
    left: 0;
    height: 45px;
    box-sizing: content-box;
    background: #fff;
    z-index: 999;
  }
  .title-container {
    height: 44px;
    display: flex;
    align-items: center;
    position: relative;
  }
  .capsule {
    margin-left: 10px;
    height: 32px;
    border: 1px solid #e8e8e8;
    border-radius: 16px;
    display: flex;
    align-items: center;
  }
  .capsule > view {
    width: 32px;
    height: 60%;
    position: relative;
  }
  .capsule > view:nth-child(2) {
    border-left: 1px solid #e8e8e8;
  }
  .capsule image {
    width: 60%;
    height: 100%;
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate(-50%,-50%);
  }
  .title {
    color: white;
    position: absolute;
    left: 104px;
    right: 104px;
    font-size: 14px;
    text-align: center;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
  }